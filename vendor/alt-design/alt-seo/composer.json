{
    "name": "alt-design/alt-seo",
    "description": "Alt SEO addon, add SEO tags to your site",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AltDesign\\AltSeo\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0"
    },
    "extra": {
        "statamic": {
            "name": "Alt Seo",
            "description": "Alt Seo addon"
        },
        "laravel": {
            "providers": [
                "AltDesign\\AltSeo\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false
        }
    }
}
